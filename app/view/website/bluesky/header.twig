<section id="hed">
    <div class="ui vertical masthead segment">
        <div class="ui container nav">

            <nav id="mnav" class="ui medium secondary pointing menu">
                <a class="header item logo"
                   onclick="gtag('event','Logo',{'event_category':'Navigation','event_label':'Header Main'});" href="/"
                   title="BlueSky Digital Labs home"><img src="{{ img }}logo.jpg"
                                                          alt="BlueSky Perth Custom Web + App Development"/></a>
                <div class="right menu">

                    {% set nav = buildNav(1) %}
                    {% for i in nav %}
                        {% if i.id != 1 %}
                            {% if i.is_navigation %}
                                <a class="item{% if (_route._handler._app == "page" and (item.parent_id == i.id)) or (_route._handler._app == "directory" and i.id == 3) or (_route._handler._app == "staff" and i.id == 4) or (_route._handler._app == "blog" and i.id == 5) %} active{% endif %}"
                                   href="{{ i._url }}" title="{{ i.title }}">{{ i.title }}</a>
                            {% endif %}
                        {% endif %}
                    {% endfor %}
                    <a class="tel item" href="tel:+1300001815"
                       onclick="gtag('event','Call',{'event_category':'Navigation','event_label':'Header Main'});"
                       title="Ring a BlueSky representative"><i class="phone volume icon"></i>1300 001 815</a>
                    <a class="toc item"><i class="sidebar icon"></i></a>

                </div><!-- /.right.menu -->
            </nav><!-- /#mnav -->
        </div><!-- /.ui.container.nav -->

        <div class="emspace h80 mtop"></div>

    </div><!-- /.masthead.segment -->
</section><!-- /#hed -->

