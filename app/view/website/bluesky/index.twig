<link rel="apple-touch-icon" sizes="180x180" href="{{ img }}_ico/apple-icon-180x180.png">
<link rel="icon" type="image/png" sizes="192x192"  href="{{ img }}_ico/android-icon-192x192.png">
<link rel="icon" type="image/png" sizes="32x32" href="{{ img }}_ico/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="96x96" href="{{ img }}_ico/favicon-96x96.png">
