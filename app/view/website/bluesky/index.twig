<!DOCTYPE html>
<html amp lang="en">
<head>
<meta charset="utf-8">
<!--base href="{{ _url.base }}"-->
{% set site = 'Blue Sky Digital Labs' %}
<title>{% if item.browser_display and item.browser_display != item.name %}{{ item.browser_display }}{% else %}{% if item.name %}{{ item.name }}{% elseif _route.tag %}{{ _route.tag }}{% elseif _route._route %}{{ _route._route }}{% else %}404{% endif %} [ {{ site }} ] Perth based marketing, digital and creative consultancy with a Digital First approach{% endif %}</title>
<meta name="description" content="{% if item.seo_description %}{{ item.seo_description }}{% else %}{% if item.summary %}{{ item.summary }}{% elseif item.description %}{{ item.description|raw|striptags|length > 256 ? item.description|raw|striptags|slice(0,254) ~ '..' : item.discription|raw|striptags }}{% else %}{{ site }}. {{ item.title }}{% endif %}{% endif %}">
<!--{#{% if page.seo_keywords %}<meta name="keywords" content="{{ page.seo_keywords }}">{% endif %}#}-->

<meta property="og:type" content="{% if item.is_home_page %}website{% else %}article{% endif %}" />
<meta property="og:title" content="{% if item.browser_display and item.browser_display != item.name %}{{ item.browser_display }}{% else %}{% if item.name %}{{ item.name }}{% elseif _route.tag %}{{ _route.tag }}{% elseif _route._route %}{{ _route._route }}{% else %}404{% endif %}{% endif %}" />
<meta property="og:description" content="{% if item.seo_description %}{{ item.seo_description }}{% else %}{% if item.summary %}{{ item.summary }}{% elseif item.description %}{{ item.description|raw|striptags|length > 256 ? item.description|raw|striptags|slice(0,254) ~ '..' : item.discription|raw|striptags }}{% else %}{{ site }}. {{ item.title }}{% endif %}{% endif %}" />
<meta property="og:url" content="{{ _route.url|trim('/') }}" />
<meta property="og:site_name" content="{{ site }}" />
{% if item.featured_image %}
<meta property="og:image" content="{{ _url.base|trim('/') }}{{ item.featured_image.reference }}" />
{% endif %}

<!-- site by Sidra Blue | http://sidrablue.com.au/ -->
<link rel="canonical" href="{{ _route.url|trim('/') }}">
<meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">

{% set img = '/app/website/bluesky/img/' %}
