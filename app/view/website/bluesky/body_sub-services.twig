
{% if not item.is_home_page %}
{% set cb = getContent(5) %}
{{ renderContent(cb.content)|raw }}
{% endif %}

<div class="emspace h20 floatc"></div>

<div class="ui four doubling centered cards noborder">

{% set subnav = buildNav(1, 2) %}
{% for i in subnav %}
{% if i.is_navigation %}
{% if i.id != item.id %}

	<a class="card" href="{{ i._url }}" title="{{ i.title }}">
		<div class="image">
			<img src="{% if i.featured_image %}{{ imgUrl(i.featured_image.reference) }}{% else %}{{ img }}iso/{% if i.id == 7 %}hero-sub_startup.png{% elseif i.id == 8 %}hero-sub_system.png{% elseif i.id == 9 %}hero-sub_app.png{% elseif i.id == 10 %}hero-sub_data.png{% elseif i.id == 11 %}hero-sub_custom.png{% elseif i.id == 12 %}hero-sub_ideas.png{% elseif i.id == 13 %}hero-sub_digital.png{% elseif i.id == 21 %}hero-sub_beacons.png{% else %}hero.svg{% endif %}{% endif %}">
		</div>
		<div class="content">
			<div class="header">{{ i.title }}</div>
			<div class="description">{{ i.summary|striptags }}</div>
		</div>
	</a><!-- /.card -->

{% endif %}
{% endif %}
{% endfor %}

</div><!-- /.ui.four.doubling.centered.cards -->

<div class="emspace h20 floatc"></div>

{% if not item.is_home_page %}
{% set cb = getContent(6) %}
{{ renderContent(cb.content)|raw }}
<div class="ui divider hidden"></div>
<p><a class="ui primary button" href="/contact" title="">Schedule a meeting <i class="right chevron icon"></i></a></p>
{% endif %}

